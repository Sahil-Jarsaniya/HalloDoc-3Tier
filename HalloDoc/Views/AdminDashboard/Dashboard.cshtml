@*Clear Case*@

<div class="modal fade" id="ClearCaseModel" tabindex="-1" aria-labelledby="newReqModal" aria-hidden="true">
    <div class="modal-dialog modal-dialog-centered">
        <div class="modal-content" id="modalContent">
            <div class="modal-body d-flex flex-column align-items-center justify-content-center">
                <input id="ClearClientId" value="" hidden />
                <br />
                <svg xmlns="http://www.w3.org/2000/svg" width="80" height="80" fill="white"
                     class="bi bi-exclamation-circle" viewBox="0 0 16 16"
                     style="background-color: #ffc107;border:3px #ffc107 solid;   border-radius: 100%;">
                    <path d="M8 15A7 7 0 1 1 8 1a7 7 0 0 1 0 14m0 1A8 8 0 1 0 8 0a8 8 0 0 0 0 16" />
                    <path d="M7.002 11a1 1 0 1 1 2 0 1 1 0 0 1-2 0M7.1 4.995a.905.905 0 1 1 1.8 0l-.35 3.507a.552.552 0 0 1-1.1 0z" />
                </svg>
                <br />
                <h4>Confirmation for clear case</h4>
